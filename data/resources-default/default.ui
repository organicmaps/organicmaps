<root>
  <ruler>
    <portrait>
      <anchor vertical="center" horizontal="right"/>
      <relative vertical="bottom" horizontal="right"/>
      <offset x="70" y="10"/>
    </portrait>
    <landscape>
      <anchor vertical="center" horizontal="right"/>
      <relative vertical="bottom" horizontal="right"/>
      <offset x="70" y="10"/>
    </landscape>
  </ruler>
  <compass>
    <portrait>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="bottom" horizontal="left"/>
      <offset x="35" y="90"/>
    </portrait>
    <landscape>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="bottom" horizontal="left"/>
      <offset x="35" y="90"/>
    </landscape>
  </compass>
  <copyright>
    <portrait>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="top" horizontal="center"/>
      <offset x="0" y="30"/>
    </portrait>
    <landscape>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="top" horizontal="center"/>
      <offset x="0" y="30"/>
    </landscape>
  </copyright>
  <country_status>
    <portrait>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="center" horizontal="center"/>
      <offset x="0" y="0"/>
    </portrait>
    <landscape>
      <anchor vertical="center" horizontal="center"/>
      <relative vertical="center" horizontal="center"/>
      <offset x="0" y="0"/>
    </landscape>
  </country_status>
</root>
